@model List<WebApplication4.Models.User>
<html>
<head>
    <link type="text/css" rel="stylesheet" href="~/css/table.css" />
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet">
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.bundle.min.js"></script>
</head>
<body>
    <table id="user">
        <tr>
            <th>Name</th>
            <th>Email</th>
            <th>Password</th>
            <th>Delete</th>
        </tr>
        @foreach (WebApplication4.Models.User u in Model)
        {

     
    <tr>
        <form asp-action="UserList" method="post">
        <td><input disabled type="text" name="Name" value="@u.Name" /></td>
        <td><input disabled type="email" name="Email" value="@u.Email" /></td>
        <td><input disabled type="password" name="Password" value="@u.Password" /></td>
        <td><button type="submit" class="btn btn-danger" id="del">Block User</button></td>
        </form>
    </tr>


    }
    </table>
</body>
</html>