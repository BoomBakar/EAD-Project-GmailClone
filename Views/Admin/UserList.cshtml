@model List<WebApplication4.Models.User>
<html>
<head>
    <link type="text/css" rel="stylesheet" href="~/css/table.css" />
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet">
</head>
<body>
    <table id="user">
        <tr>
            <th>Name</th>
            <th>Email</th>
            <th>Password</th>
            <th>Delete</th>
        </tr>
        
        @foreach (WebApplication4.Models.User user in Model)
        {

            <tr>
                <form method="post" action="/Admin/UserList">
                <td><input disabled type="text" name="Name" value="@user.Name"/></td>
                <td><input disabled type="email" name="Email" value="@user.Email"/></td>
                <td><input disabled type="password" name="Password" value="@user.Password"/></td>
                <td><button type="submit" class="btn btn-danger">Block User</button></td>
                </form>
            </tr>
        }


    </table>
</body>
</html>
